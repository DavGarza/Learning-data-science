			$('.button.os').remove();
			if(callback) {
				callback();
			}


		}

		function getLatestVersion(callback) {
			var version = {
				date: 'Monday, March 7, 2022',
				name: '8.3.2'
			}
			if(callback){
				callback(version);
			}
		}

